{
  "name": "electronic-school-journal",
  "version": "1.0.0",
  "description": "Electronic school journal on MERN stack",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "mern",
    "react"
  ],
  "author": "Vladislav Gaponov <vlad6005@gmail.com>",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/git/commitizen.js"
    }
  },
  "devDependencies": {
    "concurrently": "7.0.0",
    "cz-customizable": "6.3.0",
    "nodemon": "2.0.15"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "config": "3.3.6",
    "express": "4.17.2",
    "express-validator": "6.14.0",
    "mongoose": "6.1.6"
  }
}
